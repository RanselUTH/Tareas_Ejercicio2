<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo_verde"
    tools:context=".MainActivity2">

    <EditText
        android:id="@+id/txtresultadoC"
        android:layout_width="380dp"
        android:layout_height="50dp"
        android:background="@drawable/borde"
        android:backgroundTintMode="src_over"
        android:ems="10"
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:textColor="#0C0C0C"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtresultadoE"
        app:layout_constraintVertical_bias="0.556" />

    <EditText
        android:id="@+id/txtresultado"
        android:layout_width="380dp"
        android:layout_height="50dp"
        android:background="@drawable/borde"
        android:backgroundTint="#F6F8FA"
        android:ems="10"
        android:foregroundGravity="fill_horizontal|clip_horizontal|fill"
        android:foregroundTint="#0E0E0E"
        android:foregroundTintMode="src_in"
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:textColor="#101010"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.425" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="380dp"
        android:layout_height="40dp"
        android:background="@drawable/borde"
        android:text="Apellidos"
        android:textAlignment="center"
        android:textColor="#0B0B0B"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/txtresultaoA"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtresultado"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/txtresultaoA"
        android:layout_width="380dp"
        android:layout_height="50dp"
        android:background="@drawable/borde"
        android:backgroundTint="#FBF8F8"
        android:ems="10"
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:textColor="#0B0A0A"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtresultado"
